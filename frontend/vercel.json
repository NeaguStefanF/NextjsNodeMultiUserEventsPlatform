{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "build": {
    "env": {
      "CI": "1",
      "NPM_CONFIG_PRODUCTION": "false"
    },
    "functions": {
      "preBuild": {
        "command": "npm install"
      },
      "build": {
        "command": "npm install --force --legacy-peer-deps && npm i --force react-render-html"
      }
    }
  }
}
